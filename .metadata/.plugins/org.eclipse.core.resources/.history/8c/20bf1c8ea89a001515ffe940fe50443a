package chapter11gamezone2;

public class Card 
{
	protected String suit;
	protected String value;
	public String getSuit()
	{
		return this.suit;
	}
	public String getValue()
	{
		return this.value;
	}
	public void setValue(String value)
	{
		this.value = value;
	}
	public void setSuit(String suit)
	{
		this.suit = suit;
	}
}
